{
  "name": "dev-ui",
  "version": "1.0.0",
  "description": "UI Packaged library with micro components for helping developers build beautiful design in react",
  "main": "dist",
  "scripts": {
    "build": "babel src -d dist",
    "build:watch": "babel src -w -d dist",
    "lint": "eslint 'src/**'; exit 0",
    "lint:fix": "eslint --fix 'src/**'; exit 0",
    "lint:watch": "esw -w src/**",
    "storybook": "start-storybook -p 2002",
    "build-storybook": "build-storybook"
  },
  "directories": {
    "src": "src",
    "assets": "assets",
    "stories": "stories"
  },
  "keywords": [
    "react",
    "ui",
    "UI library",
    "component",
    "storybook",
    "asdevs",
    "dev-ui"
  ],
  "author": "Susanta Chakraborty <susant.vanu7278@gmail.com>",
  "contributors": [
    "Susanta Chakraborty <susant.vanu7278@gmail.com> (https://github.com/susanta96)"
  ],
  "license": "MIT",
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@storybook/addon-actions": "^5.3.6",
    "@storybook/addon-info": "^5.3.6",
    "@storybook/addon-links": "^5.3.6",
    "@storybook/addon-viewport": "^5.3.6",
    "@storybook/addons": "^5.3.6",
    "@storybook/react": "^5.3.6",
    "babel-eslint": "^10.0.3",
