{
  "name": "dev-ui",
  "version": "1.0.0",
  "description": "UI Packaged library with micro components for helping developers build beautiful design in react",
  "main": "dist",
  "scripts": {
    "build": "babel src -d dist",
    "start": "babel src -w -d dist",
    "lint": "eslint 'src/**'; exit 0",
