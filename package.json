    "lint:watch": "esw -w src/**",
    "storybook": "start-storybook -p 2002",
    "build-storybook": "build-storybook"
  },
  "directories": {
    "src": "src",
    "assets": "assets",
    "stories": "stories"
  },
  "keywords": [
    "react",
    "ui",
    "UI library",
    "component",
    "storybook",
    "asdevs",
    "dev-ui"
  ],
  "author": "Susanta Chakraborty <susant.vanu7278@gmail.com>",
