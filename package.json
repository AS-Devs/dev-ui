    "bootstrap": "^4.4.1",
    "jquery": "^3.4.1",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.0",
    "eslint-watch": "^6.0.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "prop-types": "^15.7.2",
    "react-icons": "^3.8.0",
